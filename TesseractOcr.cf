ifplugin Mail::SpamAssassin::Plugin::TesseractOcr

tocr_enable 1
tocr_preprocess 1
tocr_data_dir /usr/share/tesseract-ocr/4.00/tessdata
tocr_langs eng
tocr_msg_timeout 15
tocr_img_timeout 5
tocr_skip_jpg 0
tocr_skip_png 0
tocr_skip_gif 0
tocr_skip_bmp 0
tocr_skip_tif 0
tocr_skip_pdf 0
tocr_min_size 1024
tocr_max_size 4096000
tocr_min_x 16
tocr_min_y 16
tocr_max_x 2048
tocr_max_y 2048
tocr_min_area 512
tocr_max_area 2073600

endif
